{
    "appId": "com.devagentforge.aicowork",
    "productName": "AICowork",
    "compression": "maximum",
    "files": [
        "dist-electron/**/*",
        "dist-react/**/*",
        "templateIcon.ico",
        "!**/*.map",
        "!**/*.test.*",
        "!**/*.spec.*"
    ],
    "extraResources": [],
    "npmRebuild": false,
    "asarUnpack": [
        "node_modules/better-sqlite3/build/**/*.node",
        "node_modules/@memvid/sdk/**/*.node"
    ],
    "icon": "./templateIcon.ico",
    "mac": {
        "target": "dmg"
    },
    "linux": {
        "target": "AppImage",
        "category": "Utility"
    },
    "win": {
        "signAndEditExecutable": false,
        "target": [
            "portable"
        ]
    }
}
